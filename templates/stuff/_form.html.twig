{{ form_start(form) }}
<div class="row">
    <div class="col-sm-6">{{ form_row(form.title) }}</div>
    <div class="col-sm-6">{{ form_row(form.url) }}</div>
</div>
<div class="row">
    <div class="col-sm-6">{{ form_row(form.created_at) }}</div>
    <div class="col-sm-6">{{ form_row(form.updated_at) }}</div>
</div>
<div class="row">
    <div class="col-sm-6">{{ form_row(form.user_id) }}</div>

</div>

<div class="col-sm-3">
    <button id="save" class="btn btn-success">{{ button_label|default('Save') }}</button>
</div>
{#{{ form_widget(form) }}#}
{{ form_end(form) }}
